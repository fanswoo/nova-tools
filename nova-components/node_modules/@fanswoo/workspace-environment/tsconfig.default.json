{
  "compilerOptions": {
    "allowJs": true,
    "target": "es6",
    "module": "es6",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "strict": true,
    "moduleResolution": "node",
    "lib": ["dom", "es2015", "es6"]
  },
}
