{
  "name": "@fanswoo/package-manager",
  "version": "1.7.19",
  "description": "this package can manage package",
  "main": "src/main.js",
  "scripts": {
    "start": "npm run watch:package-update",
    "watch:dependence-clone": "nodemon --watch 'src/**/*.ts' --exec ts-node -r tsconfig-paths/register -- ./src/bin/dependence-clone.ts --disable-module-alias",
    "watch:dependence-remove": "nodemon --watch 'src/**/*.ts' --exec ts-node -r tsconfig-paths/register -- ./src/bin/dependence-remove.ts  --disable-module-alias",
    "watch:package-source": "nodemon --watch 'src/**/*.ts' --exec ts-node -r tsconfig-paths/register -- ./src/bin/package-source.ts --disable-module-alias",
    "watch:package-update": "nodemon --watch 'src/**/*.ts' --exec ts-node -r tsconfig-paths/register -- ./src/bin/package-update.ts --platform npm --disable-module-alias",
    "build": "tsc --project ./",
    "dependence-clone": "node ./lib/bin/dependence-clone.js",
    "dependence-remove": "node ./lib/bin/dependence-remove.js",
    "package-source": "node ./lib/bin/package-source.js"
  },
  "bin": {
    "package-source": "./lib/bin/package-source.js",
    "package-update": "./lib/bin/package-update.js",
    "dependence-clone": "./lib/bin/dependence-clone.js",
    "dependence-remove": "./lib/bin/dependence-remove.js"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "cli-table3": "^0.6.0",
    "colors": "^1.4.0",
    "commander": "^8.2.0",
    "dotenv": "^10.0.0",
    "figlet": "^1.5.2",
    "inquirer": "^8.1.5",
    "module-alias": "^2.2.2",
    "process.argv": "^0.6.0"
  },
  "devDependencies": {
    "@types/clear": "^0.1.2",
    "@types/figlet": "^1.5.4",
    "@types/inquirer": "^8.1.2",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.9.6",
    "nodemon": "^2.0.13",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanswoo/package-manager.git"
  },
  "author": "fansWoo",
  "bugs": {
    "url": "https://github.com/fanswoo/package-manager/issues"
  },
  "homepage": "https://github.com/fanswoo/package-manager",
  "gitHead": "ef079fc04af57b795b16014af2bcd8ec494d3f17"
}
