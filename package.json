{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "production": "vite build --minify true",
        "prettier": "prettier",
        "eslint": "eslint",
        "eslint:fix": "eslint --fix",
        "test:clear": "jest --clearCache",
        "test": "jest --config jest.conf.js",
        "test:bail": "jest --bail --config jest.conf.js",
        "husky:install": "husky install",
        "package:source": "package-source --config-path resources/script/config/package-manager.js",
        "package:update": "package-update --config-path resources/script/config/package-manager.js --platform npm",
        "build-test": "cd nova-components2/Test && npm run dev",
        "build-test-prod": "cd nova-components2/Test && npm run prod",
        "build-nova-fields-display": "cd nova-components2/NovaFieldsDisplay && npm run dev",
        "build-nova-fields-display-prod": "cd nova-components2/NovaFieldsDisplay && npm run prod",
        "build-color-picker": "cd nova-components2/ColorPicker && npm run dev",
        "build-color-picker-prod": "cd nova-components2/ColorPicker && npm run prod"
    },
    "dependencies": {
        "@fanswoo/control-center": "*",
        "@fanswoo/core": "*",
        "@fanswoo/shop": "*",
        "@mojs/core": "^1.7.1",
        "@mojs/player": "^1.3.0",
        "@types/facebook-pixel": "^0.0.24",
        "aos": "^2.3.4",
        "bootstrap": "^5.3.3",
        "clipboard": "^2.0.6",
        "devtools-detect": "^4.0.0",
        "fontfaceobserver": "^2.3.0",
        "html2canvas": "^1.4.0",
        "mitt": "^3.0.1",
        "owl.carousel": "^2.3.4",
        "pako": "^2.1.0",
        "parallax-js": "^3.1.0",
        "skrollr": "^0.6.26",
        "typed.js": "^2.0.12",
        "vue-chartjs": "^5.3.1"
    },
    "devDependencies": {
        "@fanswoo/package-manager": "*",
        "@fanswoo/workspace-environment": "*"
    }
}